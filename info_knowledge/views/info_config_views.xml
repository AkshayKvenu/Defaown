<odoo>
  <data>
		
		<record model="ir.ui.view" id="view_infoknowledge_tags_tree">
			<field name="name">info.knowledge.tags.tree</field>
			<field name="model">info.knowledge.tags</field>
			<field name="arch" type="xml">
				<tree string="Info Knowledge tags" editable="bottom">
					<field name="name" required='1'/>
				</tree>
			</field>
		</record>

		<record model="ir.actions.act_window"
			id="action_infoknowledge_tags">
			<field name="name">Info Knowledge tags</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">info.knowledge.tags</field>
			<field name="view_mode">tree</field>
			<field name="help" type="html">
				<p class="o_view_nocontent_smiling_face">
					Nothing to show
				</p>
			</field>
		</record>
		
		
  </data>
</odoo>